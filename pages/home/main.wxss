    background-color: #fff;
}

.home-tab {
    width: 100%;
    height: 80rpx;
    border-bottom: 2rpx solid #e5e5e5;
    background: #fff;
}

.tab-item {
    align-items: center;
    justify-content: center;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 30rpx;
    height: 80rpx;
    position: relative;
    padding: 0 40rpx;
}

.tab-item.active {
    color: #468cee;
}

.tab-item .item-line {
    content: "";
    display: none;
    position: absolute;
    left: 20%;
    bottom: 0;
    width: 60%;
    height: 4rpx;
    background: #468cee;
}

.tab-item.active .item-line {
    display: block;
}

.home-page .home-list {
    overflow: hidden;
    padding: 0 25rpx;
    margin-top: 50rpx;
    display: none;
}

.home-page .home-list.show {
    display: block;
}

.home-page .home-list ._a:active {
    background-color: #fafbfe;
}

.home-page .home-list .home-item {
    border: 1rpx solid #d8e0f5;
    padding: 40rpx 20rpx;
    text-align: center;
    float: left;
    margin-bottom: 50rpx;
    margin-left: 25rpx;
    margin-right: 25rpx;
    width: 300rpx;
    height: 260rpx;
    font-size: 30rpx;
    display: block;
    box-sizing: border-box;
    background-color: #fff;
}

.home-page .home-list .home-item:active {
    background-color: #f7f7f7;
}

.home-page .home-list .home-item-icon {
    width: 100rpx;
    height: 100rpx;
    display: block;
    margin: auto auto 18rpx;
}

