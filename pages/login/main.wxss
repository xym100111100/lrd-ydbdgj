./static/css/vendor.wxss.versionOn {
    margin-top: 80rpx;
    color: #acacac;
    font-size: 24rpx;
    text-align: center;
}

.login-page {
    display: flex;
    flex-direction: column;
    height: 100%;
    background: #fff;
}

.login-page .section {
    padding: 50rpx;
}

.login-page .logo-item {
    text-align: center;
    margin-bottom: 25rpx;
    margin-top: 129rpx;
}

.login-page .logo-item .logo-img {
    background-image: url(data:image/png;base64,);
    background-size: cover;
    width: 200rpx;
    height: 200rpx;
    margin-bottom: 18rpx;
    margin-left: auto;
    margin-right: auto;
}

.login-page .logo-item ._p {
    font-size: 36rpx;
    color: #000;
}

.login-page .section .input-group {
    align-items: center;
    border: 1rpx solid #bcbcbc;
    display: flex;
    margin-bottom: 30rpx;
    position: relative;
}

.login-page .section ._button:after {
    border: none;
    border-radius: none;
}

.login-page .section .code-btn {
    flex: 1;
    color: #487abd;
    background: #fff;
    border: none;
    border-radius: 0;
    padding: 14rpx 0;
    font-size: 29rpx;
    width: 234rpx;
    text-align: center;
    margin-right: 1rpx;
}

.login-page .section .code-btn[disabled] {
    color: #acacac;
}

.login-page .section ._input.smsCode {
    flex: 2;
}

.login-page .section ._input {
    flex: 1;
    width: auto;
    padding: 28rpx 0 28rpx 30rpx;
    font-size: 29rpx;
    color: #212121;
}

.login-page .btn-area {
    margin-top: 60rpx;
}

.login-page .btn-area ._button:after {
    content: "";
    border-radius: 0;
    border: none;
}

.login-page .btn-area ._button {
    background-color: #487abd;
    border: 1rpx solid #487abd;
    border-radius: 0;
    font-size: 36rpx;
    padding: 25rpx;
    line-height: 50rpx;
}

.login-page .btn-area ._button[disabled][type=primary] {
    background-color: rgba(72,122,189,.5);
    border: none;
}